	    <div class="row-fluid statistics">
	      <div class="span4">
		<!-- linediv-l and linediv-r give dividing lines that look different in horizontal and vertical layouts, illustrating media queries. -->
		<div class="linediv-l">
		  <% var backers = 0; var bc_sum = 0; %>
		  <% for(var i=0; i<orders.length; i++) {%>
		     <% backers = backers + 1 %>
		     <% bc_sum = bc_sum + orders[i].amount %>
		  <% } %>
		<h3><%= backers %></h3> <p>backers</p>
		</div>
	      </div>
	         <div class="span4">
		<div class="linediv-c">
		  <h3><%= bc_sum * 126 %></h3> <p>of $10,000</p>
		</div>
	      </div>
	      <div class="span4">
		<div class="linediv-r">
		<h3 id="countdown"></h3> <p>days left</p>
		</div>
	      </div>
	    </div>
	  </div>
	  <div class="row-fluid">
